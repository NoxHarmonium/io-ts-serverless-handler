{
  "name": "io-ts-serverless-handler",
  "version": "2.0.0",
  "description": "A simple wrapper for Serverless Framework HTTP handlers to remove the boilerplate of using io-ts codecs to validate and extract request parameters",
  "main": "lib/index.js",
  "module": "es6/index.js",
  "typings": "lib/index.d.ts",
  "sideEffects": false,
  "author": "Sean Dawson <contact@seandawson.info>",
  "repository": {
    "type": "git",
    "url": "https://github.com/NoxHarmonium/io-ts-serverless-handler"
  },
  "license": "Apache-2.0",
  "peerDependencies": {
    "fp-ts": "^2.4.1",
    "io-ts": "^2.0.5"
  },
  "scripts": {
    "build": "tsc && tsc -p tsconfig.es6.json",
    "test": "jest --coverage",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lint-fix": "tslint --fix -p tsconfig.json -c tslint.json",
    "format-check": "prettier-check src/**/*",
    "type-coverage": "type-coverage --strict --detail --ignore-catch",
    "format": "prettier --write '{src,test}/**/*.{ts,tsx}'",
    "quality-check": "yon test && yon lint && yon format-check && yon type-coverage",
    "semantic-release": "semantic-release",
    "prepare": "yon build"
  },
  "devDependencies": {
    "@codechecks/build-size-watcher": "^0.1.0",
    "@codechecks/client": "^0.1.10",
    "@jedmao/semantic-release-npm-github-config": "^1.0.9",
    "@types/aws-lambda": "^8.10.39",
    "@types/jest": "^24.0.25",
    "fp-ts": "^2.4.1",
    "io-ts": "^2.0.5",
    "io-ts-types": "^0.5.6",
    "jest": "^25.1.0",
    "jest-each": "^25.1.0",
    "monocle-ts": "^2.1.0",
    "newtype-ts": "^0.3.4",
    "prettier": "^1.19.1",
    "prettier-check": "^2.0.0",
    "semantic-release": "^17.0.4",
    "ts-jest": "^25.2.1",
    "tslint": "^6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-immutable": "^6.0.1",
    "tslint-sonarts": "^1.9.0",
    "type-coverage": "^2.4.0",
    "typecov": "^0.2.1",
    "typescript": "^3.7.4",
    "yarn-or-npm": "^3.0.1"
  },
  "resolutions": {
    "handlebars": "4.5.0"
  },
  "dependencies": {
    "type-fest": "^0.11.0"
  },
  "typeCoverage": {
    "atLeast": 100
  },
  "files": [
    "/es6",
    "/lib"
  ]
}
